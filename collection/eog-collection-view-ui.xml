<Root>
  
<commands>
        <cmd name="ImagePrev" _label="Previous Image"
             _tip=""/>
        <cmd name="ImageNext" _label="Next Image"
             _tip=""/>
        <cmd name="SlideShow" _label="Slide Show" 
             _tip="" accel="F11"/>
</commands>

<menu>

    <submenu name="View" _label="_View">

            <placeholder name="View Items Placeholder">
  	             <menuitem name="SlideShow" verb=""/>
            </placeholder>

 	    <placeholder name="Zoom Items Placeholder" delimit="top"/>
    </submenu>

</menu>

<dockitem name="Toolbar">
    <placeholder name="Extra Buttons Placeholder">
            <toolitem name="ImagePrev" _label="Previous" verb=""
                      pixtype="stock" pixname="gtk-go-back"/>
            <toolitem name="ImageNext" _label="Next" verb=""
	              pixtype="stock" pixname="gtk-go-forward"/>
    </placeholder>
</dockitem>

</Root>
